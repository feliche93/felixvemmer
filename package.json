{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm concurrently \"content-collections watch\" \"next dev\"",
    "build": "pnpm content-collections build && next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check . --fix --unsafe",
    "format": "biome format . --write",
    "typecheck": "tsc --pretty --noEmit",
    "generate": "drizzle-kit generate --config=drizzle.config.ts",
    "generate-custom": "drizzle-kit generate --custom --config=drizzle.config.ts",
    "migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "studio": "drizzle-kit studio --config=drizzle.config.ts",
    "update-pg": "drizzle-kit up:pg --config=drizzle.config.ts",
    "check": "drizzle-kit check:pg --config=drizzle.config.ts",
    "drop": "drizzle-kit drop --config=drizzle.config.ts",
    "generate-content": "pnpm content-collections build",
    "generate-posthog-sdk": "pnpm openapi-typescript-codegen --input ./openapi-specs/posthog.yaml --output ./src/lib/posthog-typescript-sdk --name client --useOptions --client fetch --useUnionTypes",
    "generate-papierkram-sdk": "pnpm openapi-typescript-codegen --input https://felixvemmer.papierkram.de/api/v1/api-docs/api/v1/swagger.json --output ./src/lib/papierkram-typescript-sdk --name client --useOptions --client fetch --useUnionTypes",
    "clean": "rm -rf pnpm-lock.yaml node_modules .next",
    "prepare": "husky"
  },
  "dependencies": {
    "@calcom/embed-react": "^1.5.2",
